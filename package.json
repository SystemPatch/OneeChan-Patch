    "files": {
      "metajs": "OneeChan.meta.js",
      "userjs": "OneeChan.user.js"
    },
    "min": {
      "chrome": "88",
      "firefox": "109",
      "greasemonkey": "1.15"
    }
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-bump": "0.0.17",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-compress": "^0.12.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-cssmin": "^0.6.2",
    "grunt-shell": "^1.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SystemPatch/OneeChan.git"
  },
  "scripts": {
    "build": "grunt"
  },
  "author": "seaweedchan",
  "contributors": [
    "3nly<3nly.github.com>",
