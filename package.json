{
  "name": "OneeChan",
  "version": "5.7.3",
  "description": "Customizable rice and themes for 4chan X.",
  "meta": {
    "name": "OneeChan-Patch",
    "author": "SystemPatch",
    "repo": "https://github.com/SystemPatch/OneeChan/",
    "page": "https://github.com/SystemPatch/OneeChan/",
    "downloads": "https://github.com/SystemPatch/OneeChan/raw/refs/heads/stable/builds/",
    "appid": "cnbankbnhjiakpmcjbecinckfbceeacf",
    "buildsPath": "builds/",
    "mainBranch": "stable",
    "matches": [
      "*://boards.4chan.org/*",
      "*://sys.4chan.org/*"
    ],
    "exclude_matches": [
      "*://www.4chan.org/",
      "*://www.4chan.org/*",
      "*://www.4chan.org/advertise",
      "*://www.4chan.org/advertise?*",
      "*://www.4chan.org/pass",
      "*://www.4chan.org/pass?*",
      "*://www.4chan.org/donate",
      "*://www.4chan.org/donate?*"
